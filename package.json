{
  "name": "moodle-webservices-lib",
  "version": "0.0.1",
  "description": "A library to manage moodle webservice names and parameters. Use with any moodle client.",
  "main": "./dist/main.js",
  "scripts": {
    "build": "rm -rf dist/** && babel lib --out-dir dist --ignore *.tests.js,testHelpers/*",
    "prepublish": "npm test && npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive \"lib/**/*.tests.js\"",
    "test:watch": "npm test -- --watch",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jankapunkt/moodle-js-webservices-lib.git"
  },
  "keywords": [
    "moodle",
    "lms",
    "webservices",
    "http",
    "lib"
  ],
  "author": "Jan Kuester <jkuester@uni-bremen.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jankapunkt/moodle-js-webservices-lib/issues"
  },
  "homepage": "https://github.com/jankapunkt/moodle-js-webservices-lib#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "expect": "^1.20.1",
    "mocha": "^2.5.3"
  }
}
